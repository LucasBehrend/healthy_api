{
    "version": 2,
    "builds": [
      {
        "src": "main_api/__init__.js",
        "use": "@vercel/node"
      },
      {
        "src": "conexion_cronograma_api/conexion_cronograma.js",
        "use": "@vercel/node"
      },
      {
        "src": "cronogramas/cronogramas.js",
        "use": "@vercel/node"
      },
      {
        "src": "ejemplo_api/ejemplo.js",
        "use": "@vercel/node"
      },
      {
        "src": "conexion_ejemplo_api/conexion_ejemplo.js",
        "use": "@vercel/node"
      }
      
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main_api/__init__.js"
        },
        {
            "src": "/conexion_cronograma/(.*)",
            "dest": "conexion_cronograma_api/conexion_cronograma.js"
        },
        {
            "src": "/cronograma/(.*)",
            "dest": "cronogramas/cronogramas.js"
        },
        {
            "src": "/ejemplo/(.*)",
            "dest": "ejemplo/ejemplo.js"
        },
        {
            "src": "/conexion_ejemplo/(.*)",
            "dest": "conexion_ejemplo_api/conexion_ejemplo.js"
        }
    ],
    "buildCommand": "node main_api/__init__.js && node conexion_cronograma_api/conexion_cronograma.js && node cronogramas/cronogramas.js && node ejemplo_api/ejemplo.js && node conexion_ejemplo_api/conexion_ejemplo.js"

  }