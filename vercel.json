{
    "version": 2,
    "builds": [
      {
        "src": "main_api/__init__.js",
        "use": "@vercel/node"
      },
      {
        "src": "conexion_cronograma_api/conexion_cronograma.js",
        "use": "@vercel/node"
      },
      {
        "src": "cronogramas/cronogramas.js",
        "use": "@vercel/node"
      },
      {
        "src": "ejemplo_api/ejemplo.js",
        "use": "@vercel/node"
      },
      {
        "src": "conexion_ejemplo_api/conexion_ejemplo.js",
        "use": "@vercel/node"
      }
      
    ],
    "routes": [
        {
            "src": "/main_api/(.*)",
            "dest": "main_api/__init__.js"
        },
        {
            "src": "/cronograma/(.*)",
            "dest": "conexion_cronograma_api/conexion_cronograma.js"
        },
        {
            "src": "/cronogramas/(.*)",
            "dest": "cronogramas/cronogramas.js"
        },
        {
            "src": "/ejemplo/(.*)",
            "dest": "ejemplo/ejemplo.js"
        },
        {
            "src": "/conexion-ejemplo/(.*)",
            "dest": "conexion_ejemplo_api/conexion_ejemplo.js"
        },
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
  }